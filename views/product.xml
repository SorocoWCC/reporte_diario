<?xml version="1.0" encoding="UTF-8"?>
<odoo>
   <data>

    <!-- Vista de formulario - Heredada - Product -->
    <record model="ir.ui.view" id="product_template_reporte_diario_view_form">
      <field name="name">Inherit Form View - Module: Reporte Diario - Object: Product Template</field>
      <field name="model">product.template</field>
      <field name="inherit_id" ref="product.product_template_only_form_view"/>
      <field name="arch" type="xml">
        <notebook position="inside" >
          <page string="Reporte Diario" groups="base.group_system">
            <group>
              <field  name="incluir_reporte" />
              <field  name="precio_venta_reporte"/>
            </group>           
          </page>
        </notebook>
      </field>
    </record>
  </data>
</odoo>